#include <stdio.h>
#include <cs50.h>
int Height = 16;
char StartC = ' ';
char EndC = '#';
int main(void){
    Height = get_int("Enter Height: ");
    for(int x = Height; x>=0; x--){
        for(int i = 0; i < x; i++)
            printf("%c", StartC);
        for(int i = Height; i >= x; i--)
            printf("%c", EndC);
        printf("\n");
    }
}
